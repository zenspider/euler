   3.5290407779633923`*^9, {3.529041447863562*^9, 3.529041470033041*^9}}],

Cell[BoxData[
 RowBox[{"{", 
  RowBox[{"1", ",", "2", ",", "4", ",", "7", ",", "14", ",", "28"}], 
  "}"}]], "Output",
 CellChangeTimes->{{3.5290406289900084`*^9, 3.529040634854054*^9}, 
   3.5290407813885593`*^9}]
}, Closed]],

