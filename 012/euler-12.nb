            RowBox[{"Range", "[", "n", "]"}]}], "]"}], "]"}], ">", "100"}], 
        ",", 
        RowBox[{
         RowBox[{"a", "=", "n"}], ";", 
         RowBox[{"Break", "[", "]"}]}]}], "]"}], ",", 
      RowBox[{"{", 
       RowBox[{"n", ",", "1000"}], "}"}]}], "]"}], ";", 
